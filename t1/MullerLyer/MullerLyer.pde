void setup() {
  size(800, 800);  
}  


void draw() {
  if (mousePressed) {
    stroke(255);
    line(200, 300, 250, 350);
    line(200, 300, 250, 250);
    line(600, 300, 550, 250);
    line(600, 300, 550, 350);  
    line(200, 200, 150, 250);
    line(200, 200, 150, 150);
    line(600, 200, 650, 150);  
    line(600, 200, 650, 250);      
  } else {
    stroke(0);
    strokeWeight(4);
    
    line(200, 200, 600, 200);
    line(200, 200, 150, 250);
    line(200, 200, 150, 150);
    line(600, 200, 650, 150);  
    line(600, 200, 650, 250);      
    
    line(200, 300, 600, 300);
    line(200, 300, 250, 350);
    line(200, 300, 250, 250);
    line(600, 300, 550, 250);
    line(600, 300, 550, 350);    
    

  }
}
